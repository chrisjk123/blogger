{
    "name": "chriscreates/blogger",
    "description": "Blog package",
    "keywords": [
        "chriscreates",
        "blogger"
    ],
    "homepage": "https://github.com/chriscreates/blogger",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Christopher Kelker",
            "email": "christopherjk123@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chriscreates\\Blogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chriscreates\\Blogger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chriscreates\\Blogger\\BloggerServiceProvider"
            ],
            "aliases": {
                "Blogger": "Chriscreates\\Blogger\\BloggerFacade"
            }
        }
    }
}
