{
    "name": "chrisjk123/laravel-blogger",
    "description": "blog package",
    "keywords": [
        "chrisjk123",
        "laravel",
        "laravel-blogger",
        "blog",
        "blogger"
    ],
    "homepage": "https://github.com/chrisjk123/laravel-blogger",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Christopher Kelker",
            "email": "christopherjk123@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "~5.8|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chriscreates\\Blog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chriscreates\\Blog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chriscreates\\Blog\\BloggerServiceProvider"
            ]
        }
    }
}
